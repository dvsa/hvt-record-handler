    "dotenv": "^8.2.0",
    "joi": "^17.4.0",
    "light-date": "^1.2.0",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@dvsa/eslint-config-ts": "^3.0.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/deep-equal": "^1.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
